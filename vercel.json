{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "src": "api/auth(.*)",
            "dest": "/api/index.js"
        },
        {
            "src": "api/user(.*)",
            "dest": "/api/index.js"
        },
        {
            "src": "api/post(.*)",
            "dest": "/post/index.js"
        }
    ]
}